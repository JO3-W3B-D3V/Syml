{
  "name": "syml",
  "version": "1.0.0",
  "description": "Syml is a lightweight & very easy to use state managment tool.",
  "main": "index.js",
  "scripts": {
    "build": "npx webpack",
    "build:watch": "npm run build --watch",
    "test": "npm run build && npx mocha \"public/test.js\"",
    "coverage": "nyc --reporter=lcov --compact=true --temp-dir=coverage --include=public --exclude=node_modules npm run test \"public/test.js\"",
    "test:watch": "concurrently --kill-others \"npm run build:watch\" \"live-server --port=9000 --mount=/:public\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JO3-W3B-D3V/Syml.git"
  },
  "keywords": [
    "state"
  ],
  "author": "Joseph Evans",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JO3-W3B-D3V/Syml/issues"
  },
  "homepage": "https://github.com/JO3-W3B-D3V/Syml#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.2",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "live-server": "^1.2.1",
    "mocha": "^6.2.0",
    "mocha-webpack": "^1.1.0",
    "npx": "^10.2.0",
    "nyc": "^14.1.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
